<h1>Welcome to your Home Page!</h1>
<h3>This page displays all the users, cats, and todos in your database.</h3>
<br>
<br>
<br>

<h4>Users</h4>
<table>                         
  <tr>                          
    <th>Username</th>
    <th>Email</th>
    <th>Age</th>
  </tr>
<% @all_users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.age %></td>
  </tr>
<% end %>
</table> 
<br>
<br>

<h4>Cats</h4>
<table>                         
  <tr>                          
    <th>Name</th>
  </tr>
<% @all_cats.each do |cat| %>
  <tr>
    <td><%= cat.name %></td>
  </tr>
<% end %>
</table> 
<br>
<br>

<h4>Todos</h4>
<table>                         
  <tr>                          
    <th>Tasks</th>
    <th>Finished?</th>
  </tr>
<% @all_todos.each do |todo| %>
  <tr>
    <td><%= todo.tasks %></td>
    <td><%= todo.finished %></td>
  </tr>
<% end %>
</table> 
<br>
<br>